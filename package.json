{
  "name": "sa-api",
  "version": "1.0.0",
  "description": "",
  "main": "-r ./dist/alias.js ./dist/index.js",
  "scripts": {
    "watch": "tsc -w",
    "watchstart": "nodemon -r .\\dist\\alias.js .\\dist\\index.js",
    "start": "node -r ./dist/alias.js ./dist/index.js",
    "build": "npm run clean && npm run compile && npm run copy-files",
    "clean": "rm -rf dist || del /S /Q dist",
    "compile": "tsc -b ./tsconfig.json",
    "copy-files": "cp ./src/index.html ./dist/ || copy /B .\\src\\index.html .\\dist\\"
  },
  "repository": {
    "type": "git",
    "url": "sa-api"
  },
  "author": "Mass",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^4.17.17",
    "axios": "^1.4.0",
    "body-parser": "^1.19.2",
    "copyfiles": "^2.4.1",
    "express": "^4.17.17",
    "express-rate-limit": "^6.0.0",
    "mysql2": "^3.5.1",
    "node-cache": "^5.1.2",
    "socket.io-client": "^4.7.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "tsconfig-paths": "^4.2.0"
  }
}
